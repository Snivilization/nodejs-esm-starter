{
  "include": "src",
  "exclude": "test",
  "extension": ".ts",
  "require": [
    "ts-node/register",
    "source-map-support/register"
  ],
  "reporter": [
    "text-summary",
    "html"
  ],
  "sourceMap": true,
  "instrument": true,
  "check-coverage": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "report-dir": "./coverage",
  "skip-full": true
}