{
  "name": "nodejs-esm-starter",
  "version": "1.0.0",
  "description": "Starter project for NodeJs esm packages, with esbuild, typescript, mocha, chai, eslint, istanbul/nyc, gulp",
  "type": "module",
  "main": "src/main.js",
  "exports": {
    ".": {
      "require": "./lib/main.cjs",
      "import": "./src/main.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build": "rollup -c && cpr src/text.txt lib/ --overwrite",
    "test": "node test/tryout.js && node test/tryout.cjs"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Snivilization/nodejs-esm-starter.git"
  },
  "keywords": [
    "esm",
    "nodejs"
  ],
  "author": "plastikfan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Snivilization/nodejs-esm-starter/issues"
  },
  "homepage": "https://github.com/Snivilization/nodejs-esm-starter#readme",
  "devDependencies": {
    "cpr": "^3.0.1",
    "rollup": "^2.58.0"
  }
}